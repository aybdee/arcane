{
  "name": "arcane",
  "displayName": "Arcane",
  "description": "Language support for the arcane programming language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "arcane",
      "aliases": ["Arcane", "arcane"],
      "extensions": [".arc"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "arcane",
      "scopeName": "main.arc",
      "path": "./syntaxes/arcane.tmLanguage.json"
    }]
  }
}
